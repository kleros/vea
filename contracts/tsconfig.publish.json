{
  "compilerOptions": {
    "target": "es2021",
    "module": "commonjs",
    "strict": true,
    "esModuleInterop": true,
    "outDir": "typechain-types",
    "declaration": true,
    "sourceMap": true,
    "noImplicitAny": false,
    "resolveJsonModule": true,
    "skipLibCheck": true
  },
  "include": [
    "./typechain-types"
  ],
  "exclude": [
    "node_modules",
    "dist"
  ]
}
